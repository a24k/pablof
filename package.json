{
  "name": "pablof",
  "version": "0.1.0-mi.1.2",
  "description": "Productive Assistant for GitHub",
  "author": "Atsushi KAWASAKI",
  "private": true,
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a24k/pablof.git"
  },
  "license": "MIT",
  "main": "lib/main.js",
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run codegen && npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "3.2.1",
    "@graphql-codegen/client-preset": "2.1.0",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@types/node": "^18.15.0",
    "@typescript-eslint/parser": "^5.54.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.35.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
